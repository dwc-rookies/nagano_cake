admin/genres#index
<h2>ジャンル一・追加</h2>
  <%= form_for(@genre, url: admin_genres_path) do |f| %>
    <h4>ジャンル</h4>
      <%= f.text_field :name %>
    <%= f.radio_button :is_active, :無効 %>
    <%= f.label :無効, class: "is_active_label" %>
    <%= f.radio_button :is_active, :有効 %>
    <%= f.label :is_active, :有効, class: "is_active_label"%>
<%= f.submit "追加"%>
<% end %>
    
